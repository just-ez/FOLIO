{
  "extends": [
    "development"
  ],
  "hints": {
    "highest-available-document-mode": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "backdrop-filter",
          "scroll-behavior",
          "translate"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}